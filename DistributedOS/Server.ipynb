   "source": [
    "import socket\n",
    "\n",
    "s = socket.socket()\n",
    "print(\"Server created\")\n",
    "s.bind(('localhost', 9992))\n",
    "s.listen(3)\n",
    "print(\"Waiting for connection...\")\n",
    "c, addr = s.accept()\n",
    "print(f\"Connected to {addr}\")\n",
    "\n",
    "recvd_msg = c.recv(1024)\n",
    "print(f\"Received: {recvd_msg}\")\n",
    "\n",
    "c.close()\n",
    "s.close()\n"
   ]
  }
